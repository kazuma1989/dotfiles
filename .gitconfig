[user]
	name = kazuma1989
	email = kazuma.ebina@quipper.com
[alias]
	s = status
	b = branch -av
	l = log --oneline --graph
	skip = update-index --skip-worktree
	noskip = update-index --no-skip-worktree
	ca = commit --amend --no-edit
[core]
	quotepath = false
[diff]
	algorithm = histogram
[init]
	defaultBranch = main
